{
  "name": "strat-book-backend",
  "version": "1.0.0",
  "description": "The backend of StratBook.pro",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect src/index.js",
    "test": "jest",
    "deploy": "prisma deploy --env-file .env",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "8.0.0",
    "graphql": "^14.3.1",
    "graphql-cli": "^3.0.11",
    "graphql-yoga": "1.18.1",
    "jsonwebtoken": "8.5.1",
    "nodemailer": "^6.2.1",
    "nodemon": "1.19.1",
    "npm-run-all": "^4.1.5",
    "prisma": "1.34.0",
    "prisma-binding": "2.3.13",
    "stripe": "^7.2.0",
    "snyk": "^1.271.0"
  },
  "license": "MIT",
  "repository": {
    "url": "StratBook.pro"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "snyk": true
}
